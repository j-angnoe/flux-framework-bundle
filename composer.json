{
    "name": "fluxfx/framework-bundle",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Flux\\Framework\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        
    },
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "symfony/http-client": "^7.1",
        "pestphp/pest": "^3.8",
        "symfony/http-foundation": "^7.2",
        "pestphp/pest-plugin-drift": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
